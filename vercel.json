{
    "version": 2,
    "builds": [
        {
            "src": "api/contact.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact"
        },
        {
            "src": "/((?!api/).*)",
            "dest": "/public/$1"
        }
    ]
} 